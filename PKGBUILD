pkgname=cltotiny2
pkgver=1.06
pkgrel=2
license="GPL"
arch=('i686')
url=('http://none.com')
pkgdesc="sending Links from Console To Tinyurl.com"
depends=('xsel')
source=(cltotiny)
md5sums=('46f4b3b498e5dd4a80a2b4daaab3bfa1')
install=cltotiny.install

build() {
  cd $startdir/src
  install -D cltotiny $startdir/pkg/usr/bin/cltotiny  

} 
